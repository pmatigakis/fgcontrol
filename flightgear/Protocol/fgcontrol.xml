<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>
  <generic>
    <output>
      <line_separator>newline</line_separator>
      <var_separator>tab</var_separator>
      
      <!-- information -->
      <chunk>
        <name>Simulation time</name>
        <node>/fdm/jsbsim/simulation/sim-time-sec</node>
        <type>float</type>
        <format>%f</format>
       </chunk>
      
      <!-- position -->
      <chunk>
        <node>/position/latitude-deg</node>
        <name>Latitude</name>
        <type>float</type>
        <format>%f</format>
      </chunk>
      
      <chunk>
        <node>/position/longitude-deg</node>
        <name>Longitude</name>
        <type>float</type>
        <format>%f</format>
      </chunk>
      
      <chunk>
        <node>/position/altitude-ft</node>
        <name>Altitude</name>
        <type>float</type>
        <format>%f</format>
      </chunk>
      
      <chunk>
        <node>/position/altitude-agl-ft</node>
        <name>Altitude above ground level</name>
        <type>float</type>
        <format>%f</format>
      </chunk>
      
      <!-- orientation -->
      <chunk>
        <node>/orientation/roll-deg</node>
        <name>Roll</name>
        <type>float</type>
        <format>%f</format>
      </chunk>
      
      <chunk>
        <node>/orientation/pitch-deg</node>
        <name>Pitch</name>
        <type>float</type>
        <format>%f</format>
      </chunk>
      
      <chunk>
        <node>/orientation/heading-deg</node>
        <name>Heading</name>
        <type>float</type>
        <format>%f</format>
      </chunk>
      
      <chunk>
        <node>/orientation/alpha-deg</node>
        <name>Angle of attack</name>
        <type>float</type>
        <format>%f</format>
      </chunk>
      
      <chunk>
        <node>/orientation/side-slip-deg</node>
        <name>Side slip</name>
        <type>float</type>
        <format>%f</format>
      </chunk>
      
      <!-- velocities -->
      <chunk>
        <node>/orientation/roll-rate-degps</node>
        <name>Roll rate</name>
        <type>float</type>
        <format>%f</format>
      </chunk>
      
      <chunk>
        <node>/orientation/pitch-rate-degps</node>
        <name>Pitch rate</name>
        <type>float</type>
        <format>%f</format>
      </chunk>
      
      <chunk>
        <node>/orientation/yaw-rate-degps</node>
        <name>Yaw rate</name>
        <type>float</type>
        <format>%f</format>
      </chunk>
      
      <chunk>
        <node>/velocities/airspeed-kt</node>
        <name>Calibrated velocity</name>
        <type>float</type>
        <format>%f</format>
      </chunk>
      
      <chunk>
        <node>/velocities/vertical-speed-fps</node>
        <name>Climb rate</name>
        <type>float</type>
        <format>%f</format>
      </chunk>
      
      <chunk>
        <node>/velocities/speed-north-fps</node>
        <name>North velocity</name>
        <type>float</type>
        <format>%f</format>
      </chunk>
      
      <chunk>
        <node>/velocities/speed-east-fps</node>
        <name>East velocity</name>
        <type>float</type>
        <format>%f</format>
      </chunk>
      
      <chunk>
        <node>/velocities/speed-down-fps</node>
        <name>Vertical velocity</name>
        <type>float</type>
        <format>%f</format>
      </chunk>
      
      <chunk>
        <node>/velocities/uBody-fps</node>
        <name>u</name>
        <type>float</type>
        <format>%f</format>
      </chunk>
      
      <chunk>
        <node>/velocities/vBody-fps</node>
        <name>v</name>
        <type>float</type>
        <format>%f</format>
      </chunk>
      
      <chunk>
        <node>/velocities/wBody-fps</node>
        <name>w</name>
        <type>float</type>
        <format>%f</format>
      </chunk>
      
      <!-- accelerations -->
      <chunk>
        <node>/accelerations/pilot/x-accel-fps_sec</node>
        <name>X acceleration</name>
        <type>float</type>
        <format>%f</format>
      </chunk>
      
      <chunk>
        <node>/accelerations/pilot/y-accel-fps_sec</node>
        <name>Y acceleration</name>
        <type>float</type>
        <format>%f</format>
      </chunk>
      
      <chunk>
        <node>/accelerations/pilot/z-accel-fps_sec</node>
        <name>Z acceleration</name>
        <type>float</type>
        <format>%f</format>
      </chunk>
      
      <!-- Atmospheric data -->
      <chunk>
        <node>/environment/pressure-inhg</node>
        <name>Pressure</name>
        <type>float</type>
        <format>%04.9f</format>
      </chunk>

      <chunk>
        <node>/systems/pitot/total-pressure-inhg</node>
        <name>Total pressure</name>
        <type>float</type>
        <format>%04.9f</format>
      </chunk>

      <chunk>
        <node>/environment/temperature-degc</node>
        <name>Temperature</name>
        <type>float</type>
        <format>%04.9f</format>
      </chunk>
      
      <chunk>
            <name>Elevator</name>
            <node>/controls/flight/elevator</node>
            <type>float</type>
            <format>%f</format>
        </chunk>

        <chunk>
            <name>Aileron</name>
            <node>/controls/flight/aileron</node>
            <type>float</type>
            <format>%f</format>
        </chunk>

        <chunk>
            <name>Rudder</name>
            <node>/controls/flight/rudder</node>
            <type>float</type>
            <format>%f</format>
        </chunk>
        
        <chunk>
            <name>Throttle</name>
            <node>/controls/engines/engine/throttle</node>
            <type>float</type>
            <format>%f</format>
        </chunk>
    </output>
    
   	<input>
    	<line_separator>newline</line_separator>
       	<var_separator>tab</var_separator>

       	<chunk>
            <name>Elevator</name>
            <node>/controls/flight/elevator</node>
            <type>float</type>
            <format>%f</format>
        </chunk>

        <chunk>
            <name>Aileron</name>
            <node>/controls/flight/aileron</node>
            <type>float</type>
            <format>%f</format>
        </chunk>

        <chunk>
            <name>Rudder</name>
            <node>/controls/flight/rudder</node>
            <type>float</type>
            <format>%f</format>
        </chunk>
        
        <chunk>
            <name>Throttle</name>
            <node>/controls/engines/engine/throttle</node>
            <type>float</type>
            <format>%f</format>
        </chunk>
    </input>
  </generic>
</PropertyList>
